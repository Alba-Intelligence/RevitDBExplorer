<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:RevitDBExplorer.WPF.Converters" 
                    xmlns:DataModel="clr-namespace:RevitDBExplorer.Domain.DataModel.MemberAccessors" 
                    >

    <DataTemplate DataType="{x:Type DataModel:IMemberAccessorWithWrite}">
        <StackPanel Orientation="Horizontal">
            <Button  Content="{Binding Label.Text, Mode=OneWay}"  Command="{Binding WriteCommand}"   Margin="0 0 5 0" Padding="7 2" MinWidth="120"/>
        </StackPanel>
    </DataTemplate>
    
</ResourceDictionary>