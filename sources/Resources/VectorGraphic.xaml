    <Style x:Key="IconProperty" TargetType="Path">
        <Setter Property="Data" Value="{StaticResource vWrench}"/>
        <Setter Property="Fill" Value="black"/>
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Width" Value="10"/>
    </Style>

     <Style x:Key="IconMethod" TargetType="Path">
        <Setter Property="Data" Value="{StaticResource vCubeOutline}"/>
        <Setter Property="Fill" Value="black"/>
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Width" Value="10"/>
    </Style>
    
    <Style x:Key="IconMethodStatic" TargetType="Path">
        <Setter Property="Data" Value="{StaticResource vCube}"/>
        <Setter Property="Fill" Value="black"/>
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Width" Value="10"/>
    </Style>

    <Style x:Key="IconSelector" TargetType="Path" >        
        <Setter Property="Fill" Value="black"/>
        <Setter Property="Stretch" Value="Uniform"/>
    </Style>

    <Style x:Key="IconSelection" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vSelectionSearch}"/>       
    </Style>
    <Style x:Key="IconDb" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vDatabase}"/>       
    </Style>
    <Style x:Key="IconPickFace" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vBorderAllVariant}"/>       
    </Style>
    <Style x:Key="IconPickEdge" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vBorderTopVariant}"/>      
    </Style>
    <Style x:Key="IconLinkedElement" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vLinkVariant}"/>        
    </Style>
    <Style x:Key="IconDependentElements" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vGraph}"/>      
    </Style>
    <Style x:Key="IconActiveView" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vPanoramaOutline}"/>      
    </Style>
    <Style x:Key="IconActiveDocument" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vFileDocumentOutline}"/>        
    </Style>
    <Style x:Key="IconApplication" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vApplication}"/>      
    </Style>
    <Style x:Key="IconSchemas" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vFileCabinet}"/>       
    </Style>
    <Style x:Key="IconCategories" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vShapeOutline}"/>       
    </Style>
    <Style x:Key="IconForge" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vAnvil}"/>
    </Style>
</ResourceDictionary>
                    